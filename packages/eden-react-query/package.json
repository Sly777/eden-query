{
  "name": "@ap0nia/eden-react-query",
  "version": "1.5.0",
  "description": "elysia.js eden and react-query integration",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ap0nia/eden-query",
    "directory": "packages/eden-react-query"
  },
  "scripts": {
    "build": "tsup"
  },
  "type": "module",
  "main": "src/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "default": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.ts"
      ],
      "./*": [
        "./src/*.ts"
      ]
    }
  },
  "dependencies": {
    "@ap0nia/eden": "workspace:^",
    "@tanstack/react-query": "^5.51.16",
    "elysia": "1.1.19"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "tsup": "^8.2.3",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.51.16",
    "elysia": "1.1.19",
    "react": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
